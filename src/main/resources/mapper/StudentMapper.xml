<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spark.mapper.StudentDao">
    <select id="list" resultType="Student">
        SELECT
            id,
            name,
            age
        FROM tb_student
    </select>

    <select id="insert" parameterType="Student">
        INSERT INTO tb_studentstudent (id, name, age) VALUES (#{id}, #{name}, #{age})
    </select>


</mapper>
